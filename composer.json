{
    "name": "callcocam/tall-theme",
    "type": "package",
    "description": "The packages auto slug laravel model.",
    "keywords": ["framework", "laravel", "livewire", "alpinejs", "tailwindcss"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/jetstream": "^2.12",
        "livewire/livewire": "^2.5",
        "callcocam/tall-orm": "dev-main",
        "callcocam/tall-table": "dev-main",
        "callcocam/tall-form": "dev-main",
        "callcocam/tall-cms": "dev-main",
        "callcocam/tall-acl": "dev-main"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Tall\\Theme\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tall\\Theme\\Providers\\ThemeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
